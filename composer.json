{
    "name": "yavuzcv/filament-char-counter",
    "description": "Char counter for FilamentPHP",
    "autoload": {
        "psr-4": {
            "YavuzCv\\FilamentCharCounter\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "YavuzCv\\FilamentCharCounter\\CharCounterServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0@beta",
        "illuminate/contracts": "^9.0|^10.0",
        "livewire/livewire": "^3.0@beta",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0|^7.0",
        "orchestra/testbench": "8.14",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5|^10.0"
    }
}
